        "class rectangle:\n",
        "    def __init__(self,l,b):\n",
        "        self.l=l\n",
        "        self.b=b\n",
        "    def perimeter(self):\n",
        "        self.p=2*(self.l+self.b)\n",
        "        return(self.p)\n",
        "    def area(self):\n",
        "        self.a=self.l*self.b\n",
        "        return(self.a)"
